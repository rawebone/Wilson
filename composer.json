{
    "name": "rawebone/wilson",
    "description": "A Resource Orientated Framework for designing RESTful APIs",
    "keywords": [ "REST", "routing", "testable" ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Nick Rawe",
            "email": "rawebone@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Wilson\\": "library/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wilson\\Tests\\": "tests/"
        }
    },
    "require": {
        "symfony/http-foundation": "2.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*",
        "phpspec/prophecy-phpunit": "1.0.*"
    }
}
